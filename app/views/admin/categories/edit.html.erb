<% provide(:title, "Edit") %>
<h1>Edit</h1>

<div class="row center">
  <div class="span6 offset3">
    <%= form_for [:admin, @category] do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.text_field :name, placeholder: "Name" %>
      <%= f.submit "Save", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>